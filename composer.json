{
	"name": "builtnorth/wp-environment-indicator",
	"description": "A simple WordPress environment indicator for the admin bar",
	"type": "library",
	"version": "1.0.0",
	"license": "GPL-2.0-or-later",
	"autoload": {
		"psr-4": {
			"BuiltNorth\\WPEnvironmentIndicator\\": "inc/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"BuiltNorth\\WPEnvironmentIndicator\\Tests\\": "tests/"
		}
	},
	"authors": [
		{
			"name": "Built North",
			"homepage": "https://builtnorth.co"
		},
		{
			"name": "Dan Northern",
			"homepage": "https://dannorthern.com"
		}
	],
	"require": {
		"php": ">=8.0"
	},
	"require-dev": {
		"10up/wp_mock": "^1.0",
		"mockery/mockery": "^1.5",
		"phpunit/phpunit": "^9.0"
	},
	"scripts": {
		"test": "vendor/bin/phpunit",
		"test:coverage": "vendor/bin/phpunit --coverage-text"
	}
}
